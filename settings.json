{
  "zmq": {
    "comm_dir": "comm"
  },
  "database": {
    "url": "sqlite+aiosqlite:///database.sqlite"
  },
  "parser": {
    "url": "https://web.whatsapp.com/send?phone={0}",
    "batch_size": 2,
    "wait_interval": 5,
    "webdriver_timeout": 10,
    "photos_dir": "photos"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)-12s - %(levelname)-8s - %(message)s",
    "datefmt": "%Y-%m-%d %H:%M:%S",
    "ignored": [
      "aiosqlite",
      "aiogram.dispatcher",
      "multipart",
      "urllib3",
      "selenium"
    ]
  }
}